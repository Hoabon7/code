#include<iostream>
using namespace std;
int a[31],b[31],n,dem=100000;
long long s;
void Try(int i){
	for(int j=0;j<=1;j++){
		a[i]=j;
		if(i==n){
			long long t=0;int z=0;
			for(int k=1;k<=n;k++){
				t+=a[k]*b[k];
				if(a[k]==1) z++;
			}
			if(t==s&&z<dem&&z!=0){
				dem=z;
			}
		}
		else Try(i+1);
	}
}
int main(){
	cin>>n>>s;
	for(int i=1;i<=n;i++){
		cin>>b[i];
	}
	Try(1);
	if(dem!=0&&dem!=100000) cout<<dem;
	else cout<<-1;
	return 0;
}
